<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.addAccController">
   <children>
      <TextField fx:id="email" layoutX="40.0" layoutY="90.0" prefHeight="27.0" prefWidth="160.0" promptText="E-mail" />
      <TextField fx:id="country" layoutX="40.0" layoutY="150.0" prefHeight="27.0" prefWidth="160.0" promptText="Country" />
      <TextField fx:id="inProgram" layoutX="40.0" layoutY="212.0" prefHeight="27.0" prefWidth="160.0" promptText="In Program" />
      <TextField fx:id="name" layoutX="290.0" layoutY="90.0" prefHeight="27.0" prefWidth="160.0" promptText="Name" />
      <TextField fx:id="date" layoutX="290.0" layoutY="150.0" prefHeight="27.0" prefWidth="160.0" promptText="Date" />
      <TextField fx:id="role" layoutX="290.0" layoutY="212.0" prefHeight="27.0" prefWidth="160.0" promptText="Role" />
      <HBox layoutY="275.0" prefHeight="80.0" prefWidth="500.0">
         <children>
            <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #32CD32;" text="Save">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></Button>
            <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #FF0000;" text="Cancel">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
