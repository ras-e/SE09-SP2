<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.addAccController">
   <children>
      <TextField fx:id="email" layoutX="170.0" layoutY="111.0" prefHeight="27.0" prefWidth="160.0" promptText="Program Title" />
      <HBox layoutY="275.0" prefHeight="80.0" prefWidth="500.0">
         <children>
            <JFXButton fx:id="saveButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="250.0" stylesheets="@../../css/style.css" text="Save">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></JFXButton>
            <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="80.0" prefWidth="250.0" stylesheets="@../../css/style.css" text="Cancel">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></JFXButton>
         </children>
      </HBox>
      <DatePicker layoutX="167.0" layoutY="211.0" prefHeight="25.0" prefWidth="166.0" showWeekNumbers="true" />
      <ComboBox layoutX="170.0" layoutY="165.0" prefHeight="25.0" prefWidth="160.0" promptText="Category" />
   </children>
</AnchorPane>
