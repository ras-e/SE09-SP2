<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.addAccController">
   <children>
      <TextField fx:id="email" layoutX="170.0" layoutY="120.0" prefHeight="27.0" prefWidth="160.0" promptText="Program Title" />
      <HBox layoutY="275.0" prefHeight="80.0" prefWidth="500.0">
         <children>
            <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #32CD32;" text="Submit new info">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></Button>
            <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #FF0000;" text="Cancel">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></Button>
         </children>
      </HBox>
      <DatePicker layoutX="167.0" layoutY="211.0" prefHeight="25.0" prefWidth="166.0" showWeekNumbers="true" />
      <ComboBox layoutX="170.0" layoutY="165.0" prefHeight="25.0" prefWidth="160.0" promptText="Category" />
      <TextField fx:id="email1" layoutX="170.0" layoutY="52.0" prefHeight="27.0" prefWidth="160.0" promptText="Enter ID" />
   </children>
</AnchorPane>
