<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.addAccController">
    <children>
        <TextField fx:id="email" layoutX="40.0" layoutY="103.0" prefHeight="27.0" prefWidth="160.0" promptText="Email" />
        <TextField fx:id="country" layoutX="40.0" layoutY="145.0" prefHeight="27.0" prefWidth="160.0" promptText="Name" />
        <TextField layoutX="40.0" layoutY="194.0" prefHeight="27.0" prefWidth="160.0" promptText="Username" />
        <TextField fx:id="date" layoutX="290.0" layoutY="145.0" prefHeight="27.0" prefWidth="160.0" promptText="Account Type " />
        <TextField fx:id="role" layoutX="290.0" layoutY="194.0" prefHeight="27.0" prefWidth="160.0" promptText="Password" />
        <HBox layoutY="275.0" prefHeight="80.0" prefWidth="500.0">
            <children>
                <JFXButton fx:id="saveButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="250.0" stylesheets="@../../css/style.css" text="Save">
                    <font>
                        <Font name="System Bold Italic" size="20.0" />
                    </font></JFXButton>
                <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="80.0" prefWidth="250.0" stylesheets="@../../css/style.css" text="Cancel">
                    <font>
                        <Font name="System Bold Italic" size="20.0" />
                    </font></JFXButton>
            </children>
        </HBox>
      <Label layoutX="180.0" layoutY="43.0" prefHeight="36.0" prefWidth="141.0" text="Add Account">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
