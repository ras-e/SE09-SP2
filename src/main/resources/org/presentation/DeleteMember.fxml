<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.presentation.addAccController">
    <children>
        <TextField fx:id="email" layoutX="40.0" layoutY="90.0" prefHeight="27.0" prefWidth="160.0" promptText="Title" text="E-mail" />
        <TextField fx:id="country" layoutX="40.0" layoutY="124.0" prefHeight="27.0" prefWidth="160.0" promptText="Date" text="Name" />
        <TextField fx:id="inProgram" layoutX="40.0" layoutY="158.0" prefHeight="27.0" prefWidth="160.0" promptText="In Program" text="Program" />
        <TextField fx:id="date" layoutX="290.0" layoutY="124.0" prefHeight="27.0" prefWidth="160.0" promptText="Participants" text="Role " />
        <TextField fx:id="role" layoutX="290.0" layoutY="158.0" prefHeight="27.0" prefWidth="160.0" promptText="Category" text="Date" />
        <TextField layoutX="290.0" layoutY="91.0" prefHeight="25.0" prefWidth="160.0" text="Country" />
        <HBox layoutY="275.0" prefHeight="80.0" prefWidth="500.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #32CD32;" text="Save">
                    <font>
                        <Font name="System Bold Italic" size="20.0" />
                    </font></Button>
                <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="80.0" prefWidth="250.0" style="-fx-background-color: #FF0000;" text="Cancel">
                    <font>
                        <Font name="System Bold Italic" size="20.0" />
                    </font></Button>
            </children>
        </HBox>
      <Label layoutX="134.0" layoutY="38.0" prefHeight="35.0" prefWidth="232.0" text="Slet person fra kreditering">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
